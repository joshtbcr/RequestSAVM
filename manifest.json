{
    "manifest_version": 2,
    "name": "Request SAVM",
    "version": "0.1",
    "content_scripts":[
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery-3.5.1.min.js", "content.js"]
        }
    ],
    "browser_action":{
        "default_icon": "icon.jpg"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "web_accessible_resources": ["icon.jpg"]
}