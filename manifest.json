{
    "manifest_version": 2,
    "name": "Request SAVM",
    "version": "0.5",
    "permissions": [
        "activeTab"
      ],
    "content_scripts":[
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery-3.5.1.min.js", "content.js"]
        },
        {
            "matches": [
                "https://tdc.azure.net/*"
            ],
            "js": ["jquery-3.5.1.min.js", "contentRequestVM.js", "contentModifyHTML.js"]
        }
    ],
    "browser_action":{
        "default_icon": "favicon.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "web_accessible_resources": ["favicon.png"]
}
